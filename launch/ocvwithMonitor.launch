<launch>

  <rosparam file="$(find butia_face)/config/butia_emotions_controllers.yaml" command="load"/>

  <node pkg="butia_face" type="faceMonitoring.py" name="faceMonitoring" output="screen" />
  <node pkg="butia_face" type="eyelidController.py" name="eyelidController" output="screen" />
  <node pkg="butia_face" type="eyebrownController.py" name="eyebrowController" output="screen" />
  <node pkg="butia_face" type="emotionController.py" name="emotionController" output="screen" />
  <node pkg="butia_face" type="newNeckController.py" name="newNeckController" output="screen" />
  <node pkg="butia_face" type="dataController.py" name="dataController" output="screen" />

  <include file="$(find butia_face)/launch/mouthToTalk.launch"/>
</launch>
  
